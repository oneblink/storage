{
  "name": "@oneblink/storage",
  "description": "SDK for managing storage files in the OneBlink ecosystem",
  "version": "2.1.2-beta.3",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/storage/issues"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/lib-storage": "^3.637.0",
    "browser-or-node": "^3.0.0"
  },
  "devDependencies": {
    "@microsoft/eslint-plugin-sdl": "^0.2.2",
    "@oneblink/types": "github:oneblink/types",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fixpack": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.2.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/oneblink/storage",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/storage.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc",
    "eslint": "eslint src/**/*.ts --fix --cache",
    "fixpack": "fixpack",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "pretest": "npm run fixpack && npm run eslint && npm run typescript",
    "release": "oneblink-release repository --no-name",
    "start": "tsc-watch",
    "test": "jest",
    "types": "npm i -D github:oneblink/types",
    "typescript": "tsc --noEmit",
    "update-dependents": "oneblink-release update-dependents"
  }
}
