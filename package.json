{
  "name": "@oneblink/storage",
  "description": "SDK for managing storage files in the OneBlink ecosystem",
  "version": "4.0.0-beta.1",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/storage/issues"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.969.0",
    "@aws-sdk/lib-storage": "3.969.0",
    "browser-or-node": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@oneblink/release-cli": "^3.4.0",
    "@oneblink/types": "github:oneblink/types",
    "@types/node": "^24.10.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "fixpack": "^4.0.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "prettier-plugin-jsdoc": "^1.8.0",
    "tsc-watch": "^7.2.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/oneblink/storage",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/storage.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "pretest": "npm run fixpack && npm run eslint && npm run typescript",
    "release": "oneblink-release repository --no-name",
    "start": "tsc-watch",
    "test": "vitest run",
    "types": "npm i -D github:oneblink/types",
    "typescript": "tsc --noEmit",
    "update-dependents": "oneblink-release update-dependents --force"
  },
  "type": "module"
}
